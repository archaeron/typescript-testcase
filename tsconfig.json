{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "noEmit": false,
    "removeComments": true,
    "isolatedModules": true,
    "strictBindCallApply": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "baseUrl": "./",
    "lib": ["dom", "es2015", "es2015.core"]
  },
  "baseUrl": "./",
  "exclude": ["node_modules"],
  "include": ["testcase.ts"]
}
